<html>
<head>
<body onload="init()">
		<div id="about_lk">
			<%= link_to "About", about_path %><br>
		</div>


		<div id="btnStart">
			<canvas id="background" width="1366px" height="299px">
			      Your browser does not support canvas. Please try again with a different browser.
			</canvas>
		</div>


		<div id="texto">
				<% if @texto_lancamento %>
					<%= render partial: 'texto_lancamento' %>
				<% end %>
		</div>


		<div id="videos">
			<div>
				<% if @youtube_items %>
					<%= render partial: 'youtube_items' %>
				<% end %>
			</div>
			<div>
				<% if @vimeo_items %>
					<%= render partial: 'vimeo_items' %>
				<% end %>
			</div>
		</div>


		<div id="musicas">
			<div>
				<% if @soundcloud_items %>
					<%= render partial: 'soundcloud_items' %>
				<% end %>
			</div>
		</div>

</body>

</html>
